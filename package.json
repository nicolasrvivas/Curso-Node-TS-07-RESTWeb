  "type": "commonjs",
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^11.0.0",
    "jest": "^29.7.0",
    "prisma": "^6.18.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "env-var": "^7.5.0",
    "express": "^5.1.0"
  }
